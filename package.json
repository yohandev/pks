{
  "name": "skullhouse.mit.edu",
  "version": "1.0.0",
  "description": "Website(public + internal) for MIT's Alpha Mu chapter of Phi Kappa Sigma(Skullhouse)",
  "main": "index.js",
  "author": "Yohan Guyomard <yohang@mit.edu>",
  "license": "MIT",
  "scripts": {
    "build": "node build.js",
    "watch": "node build.js --watch",
    "serve": "yarn watch & http-server www",
    "install": "node build.js && find ./www/api -type f -iname \"*.cgi\" -exec chmod +x {} \\;"
  },
  "devDependencies": {
    "esbuild": "^0.17.19",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "google-auth-library": "^8.8.0",
    "googleapis": "^120.0.0",
    "preact": "^10.15.0",
    "preact-router": "^4.1.1"
  }
}
